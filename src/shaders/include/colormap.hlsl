float3 LinearToGamma(float3 lin) {
	return max(1.055f * pow(lin, 0.416666667f) - 0.055f, 0);
}

float3 GammaToLinear(float3 sRGB) {
	return 0.012522878f * sRGB +
	       0.682171111f * sRGB * sRGB +
	       0.305306011f * sRGB * sRGB * sRGB;
}